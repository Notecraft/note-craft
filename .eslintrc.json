{
  "env": {
    "es2021": true,
    "node": true
  },
  "extends": "eslint:recommended",
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "rules": {
    "prettier/prettier": [
      1,
      {
        "useTabs": false,
        "printWidth": 120,
        "tabWidth": 2,
        "singleQuote": true
      }
    ]
  }
}
