.title-item Tempo:
br
input#tempo-text(
  type="number",
  onKeyUp="if(this.value>600){this.value='600';}else if(this.value<30){this.value='30';}",
  step="10",
  value=currentTempo
)

.slidecontainer
  input#tempo-range.slider(
    type="range",
    min="30",
    max="600",
    step="1",
    value=currentTempo
  )
